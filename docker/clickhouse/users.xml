<?xml version="1.0"?>
<clickhouse>
    <profiles>
        <default>
            <max_memory_usage>10000000000</max_memory_usage>
            <use_uncompressed_cache>0</use_uncompressed_cache>
            <load_balancing>in_order</load_balancing>
            <log_queries>1</log_queries>
        </default>
        <readonly>
            <readonly>1</readonly>
        </readonly>
    </profiles>

    <mark_cache_size>524288000</mark_cache_size>

    <users>
        <default>
            <password></password>
            <profile>default</profile>
            <networks>
                <ip>0.0.0.0/0</ip>
            </networks>
            <quota>default</quota>
        </default>
        <chadmin>
            <password>securedb</password>
            <profile>default</profile>
            <networks>
                <ip>0.0.0.0/0</ip>
            </networks>
            <quota>default</quota>
        </chadmin>
        <api>
            <password>api</password>
            <profile>default</profile>
            <networks>
                <ip>0.0.0.0/0</ip>
            </networks>
            <quota>default</quota>
        </api>
        <worker>
            <password>worker</password>
            <profile>default</profile>
            <networks>
                <ip>0.0.0.0/0</ip>
            </networks>
            <quota>default</quota>
        </worker>
    </users>

    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>
</clickhouse>
