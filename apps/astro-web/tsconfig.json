{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"allowJs": true,
		"strictNullChecks": true,
		"types": ["vite/client"],
		"tsBuildInfoFile": ".temp/tsconfig.tsbuildinfo",
		"baseUrl": ".",
		"paths": {
			"#/*": ["src/*"]
		}
	},
	"include": [".astro/types.d.ts", "./src/**/*"],
	"exclude": ["node_modules", "dist", "public"]
}
